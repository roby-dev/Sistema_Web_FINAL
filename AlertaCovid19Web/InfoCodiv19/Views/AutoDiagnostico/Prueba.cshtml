    <script>
      //- GEOLOCALIZACION Y CHAT
      var options = {
        formatted_address: "permissionDenied"
      };
      function initMap() {
        if (navigator.geolocation) {
          navigator.geolocation.getCurrentPosition(function(position) {
            var pos = {
                lat: position.coords.latitude,
                lng: position.coords.longitude
            };
            console.log('Location found');
            options.formatted_address = "Lat:" + pos.lat + ", Lng:" + pos.lng;
            console.log(options.formatted_address);
          }, function() {
              console.log('Error');
          });
        } else {
          // Browser doesn't support Geolocation
          console.log('Error');
        }
        loadAssistant();
      }
      initMap();
      function preSendhandler(event) {
        event.data.context.skills['main skill'].user_defined.location = options.formatted_address;
      }
      function loadAssistant() {
        window.loadWatsonAssistantChat({
          integrationID: "268dc9cf-d10b-4fb2-897b-a2486c8167ca", // The ID of this integration.
          region: "us-south" // The region your integration is hosted in.
        }).then(function(instance) {
          instance.on({
            type: "pre:send",
            handler: preSendhandler
          });
          instance.render();
        });
      }
    </script>
    
