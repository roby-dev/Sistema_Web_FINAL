        font-family: 'Work Sans',sans-serif;
        font-weight: 400;
        color: #13264d;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        text-rendering: optimizeLegibility
    }

    input, select {
        border-radius: 0;
        -webkit-appearance: none;
        -moz-appearance: none;
        appearance: none
    }

        input focus, select focus {
            outline: none
        }

    u {
        text-decoration: underline
    }

    b {
        font-weight: 600
    }

    i {
        font-style: italic
    }

    a {
        text-decoration: none;
        color: #de2b5a
    }

        a:hover {
            text-decoration: none;
            color: #de2b5a
        }

        a:focus {
            text-decoration: none
        }

        a:active {
            text-decoration: none
        }

        a.btnBox {
            display: table;
            outline: none;
            margin: 0px auto 0px;
            position: relative;
            overflow: hidden;
            cursor: pointer
        }

            a.btnBox p {
                border: 2px solid #f42f63;
                padding: 12px 25px;
                text-align: center;
                -webkit-transition: all 0.3s ease 0.2s;
                -moz-transition: all 0.3s ease 0.2s;
                -o-transition: all 0.3s ease 0.2s;
                -ms-transition: all 0.3s ease 0.2s;
                transition: all 0.3s ease 0.2s;
                position: relative;
                z-index: 2
            }

                a.btnBox p:before {
                    content: '';
                    position: absolute;
                    z-index: -1;
                    width: 0;
                    height: 100%;
                    top: 0;
                    left: 0%;
                    -webkit-transition: all 0.5s ease 0.1s;
                    -moz-transition: all 0.5s ease 0.1s;
                    -o-transition: all 0.5s ease 0.1s;
                    -ms-transition: all 0.5s ease 0.1s;
                    transition: all 0.5s ease 0.1s;
                    -webkit-transform-origin: top left;
                    -moz-transform-origin: top left;
                    -o-transform-origin: top left;
                    -ms-transform-origin: top left;
                    transform-origin: top left
                }

            a.btnBox:hover {
                text-decoration: none
            }

                a.btnBox:hover p {
                    color: #fff
                }

                    a.btnBox:hover p:before {
                        width: 110%;
                        background-color: #f42f63
                    }

        a.btnBoxArrow {
            height: 60px;
            display: table;
            outline: none;
            margin: 0px auto 0px;
            position: relative;
            cursor: pointer;
            background-color: #de2b5a;
            overflow: hidden;
            -webkit-transition: all 0.3s ease 0s;
            -moz-transition: all 0.3s ease 0s;
            -o-transition: all 0.3s ease 0s;
            -ms-transition: all 0.3s ease 0s;
            transition: all 0.3s ease 0s
        }


            a.btnBoxArrow .innerContentBtn {
                padding: 18px 18px 18px 25px;
                position: relative;
                width: 100%;
                height: 100%;
                display: -webkit-box;
                display: -moz-box;
                display: -webkit-flex;
                display: -ms-flexbox;
                display: box;
                display: flex;
                -webkit-box-align: center;
                -moz-box-align: center;
                -o-box-align: center;
                -ms-flex-align: center;
                -webkit-align-items: center;
                align-items: center;
                -webkit-box-pack: center;
                -moz-box-pack: center;
                -o-box-pack: center;
                -ms-flex-pack: center;
                -webkit-justify-content: center;
                justify-content: center
            }

                a.btnBoxArrow .innerContentBtn:before {
                    content: '';
                    position: absolute;
                    z-index: 1;
                    width: 0;
                    height: 110%;
                    top: -2px;
                    left: -2px;
                    -webkit-transition: all 0.5s ease 0.1s;
                    -moz-transition: all 0.5s ease 0.1s;
                    -o-transition: all 0.5s ease 0.1s;
                    -ms-transition: all 0.5s ease 0.1s;
                    transition: all 0.5s ease 0.1s;
                    -webkit-transform-origin: top left;
                    -moz-transform-origin: top left;
                    -o-transform-origin: top left;
                    -ms-transform-origin: top left;
                    transform-origin: top left
                }

                a.btnBoxArrow .innerContentBtn p {
                    visibility: visible !important;
                    opacity: 1 !important;
                    -ms-filter: none !important;
                    filter: none !important;
                    position: relative;
                    text-align: center;
                    color: #fff;
                    font-weight: 500;
                    -webkit-transition: all 0.3s ease 0.2s;
                    -moz-transition: all 0.3s ease 0.2s;
                    -o-transition: all 0.3s ease 0.2s;
                    -ms-transition: all 0.3s ease 0.2s;
                    transition: all 0.3s ease 0.2s;
                    z-index: 2
                }

                    a.btnBoxArrow .innerContentBtn p b {
                        color: #fff;
                        -webkit-transition: all 0.3s ease 0.2s;
                        -moz-transition: all 0.3s ease 0.2s;
                        -o-transition: all 0.3s ease 0.2s;
                        -ms-transition: all 0.3s ease 0.2s;
                        transition: all 0.3s ease 0.2s;
                        font-weight: 600
                    }

                a.btnBoxArrow .innerContentBtn .arrowContent {
                    visibility: visible !important;
                    opacity: 1 !important;
                    -ms-filter: none !important;
                    filter: none !important;
                    position: relative;
                    z-index: 2;
                    padding: 2px 0 0;
                    margin-left: 15px;
                    -webkit-transition: all 0.5s ease 0s;
                    -moz-transition: all 0.5s ease 0s;
                    -o-transition: all 0.5s ease 0s;
                    -ms-transition: all 0.5s ease 0s;
                    transition: all 0.5s ease 0s
                }

                    a.btnBoxArrow .innerContentBtn .arrowContent .innerArrowContent {
                        overflow: hidden;
                        width: 10px;
                        -webkit-transition: all 0.5s ease 0s;
                        -moz-transition: all 0.5s ease 0s;
                        -o-transition: all 0.5s ease 0s;
                        -ms-transition: all 0.5s ease 0s;
                        transition: all 0.5s ease 0s
                    }

                        a.btnBoxArrow .innerContentBtn .arrowContent .innerArrowContent svg.arrow {
                            width: 18px;
                            float: right;
                            -webkit-transition: all 0.3s ease 0.2s;
                            -moz-transition: all 0.3s ease 0.2s;
                            -o-transition: all 0.3s ease 0.2s;
                            -ms-transition: all 0.3s ease 0.2s;
                            transition: all 0.3s ease 0.2s;
                            fill: #fff
                        }

                            a.btnBoxArrow .innerContentBtn .arrowContent .innerArrowContent svg.arrow .body {
                                -webkit-transition: all 0.3s ease 0s;
                                -moz-transition: all 0.3s ease 0s;
                                -o-transition: all 0.3s ease 0s;
                                -ms-transition: all 0.3s ease 0s;
                                transition: all 0.3s ease 0s;
                                opacity: 0;
                                -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
                                filter: alpha(opacity=0)
                            }

            a.btnBoxArrow:hover {
                background-color: #b71d45
            }

                a.btnBoxArrow:hover .innerContentBtn p, a.btnBoxArrow:hover .innerContentBtn span {
                    color: #fff
                }

                a.btnBoxArrow:hover .innerContentBtn .arrowContent .innerArrowContent {
                    width: 18px
                }

                    a.btnBoxArrow:hover .innerContentBtn .arrowContent .innerArrowContent svg.arrow {
                        fill: #fff
                    }

                        a.btnBoxArrow:hover .innerContentBtn .arrowContent .innerArrowContent svg.arrow .body {
                            fill: #fff;
                            opacity: 1;
                            -ms-filter: none;
                            filter: none
                        }

    html {
        overflow-y: auto !important
    }

</style>
